                                    <i class="flaticon-more"></i>
                                </a>

                                <!-- BEGIN: Topbar Toggler -->
                            </div>
                        </div>
                    </div>

                    <!-- END: Brand -->
                    <div class="m-stack__item m-stack__item--fluid m-header-head" id="m_header_nav">

                        <!-- BEGIN: Topbar -->
                        @if (Session["logon"] != null)
                        {
                            <div id="m_header_topbar" class="m-topbar  m-stack m-stack--ver m-stack--general">
                                <div class="m-stack__item m-topbar__nav-wrapper">
                                    <ul class="m-topbar__nav m-nav m-nav--inline">
                                        <li class="m-nav__item m-dropdown m-dropdown--large m-dropdown--arrow m-dropdown--align-center m-dropdown--mobile-full-width m-dropdown--skin-light	m-list-search m-list-search--skin-light" m-dropdown-toggle="click" id="m_quicksearch"
                                            m-quicksearch-mode="dropdown" m-dropdown-persistent="1">
                                            <a href="#" class="m-nav__link m-dropdown__toggle">
                                                <span class="m-nav__link-icon"><span class="m-nav__link-icon-wrapper"><i class="flaticon-search-1"></i></span></span>
                                            </a>
                                            <div class="m-dropdown__wrapper">
                                                <span class="m-dropdown__arrow m-dropdown__arrow--center"></span>
                                                <div class="m-dropdown__inner ">
                                                    <div class="m-dropdown__header">
                                                        <form class="m-list-search__form">
                                                            <div class="m-list-search__form-wrapper">
                                                                <span class="m-list-search__form-input-wrapper">
                                                                    <input id="m_quicksearch_input" autocomplete="off" type="text" name="q" class="m-list-search__form-input" value="" placeholder="Search...">
                                                                </span>
                                                                <span class="m-list-search__form-icon-close" id="m_quicksearch_close">
                                                                    <i class="la la-remove"></i>
                                                                </span>
                                                            </div>
                                                        </form>
                                                    </div>
                                                    <div class="m-dropdown__body">
                                                        <div class="m-dropdown__scrollable m-scrollable" data-scrollable="true" data-height="300" data-mobile-height="200">
                                                            <div class="m-dropdown__content">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </li>

                                        <li class="m-nav__item m-topbar__user-profile  m-dropdown m-dropdown--medium m-dropdown--arrow  m-dropdown--align-right m-dropdown--mobile-full-width m-dropdown--skin-light" m-dropdown-toggle="click">
                                            <a href="#" class="m-nav__link m-dropdown__toggle">
                                                <span class="m-topbar__userpic">
                                                    @*<img src="~/Content/images/teacher/user1.jpg" class="m--img-rounded m--marginless m--img-centered" alt="" />*@
                                                    <img src="..//@Url.Content(@Model.MEMBERS.PROFILE_IMG_URL ?? "//Content//images//Blank-Profile.jpg" )" class="m--img-rounded m--marginless m--img-centered" alt="" />
                                                </span>
                                                <span class="m-nav__link-icon m-topbar__usericon  m--hide">
                                                    <span class="m-nav__link-icon-wrapper"><i class="flaticon-user-ok"></i></span>
                                                </span>
                                                <span class="m-topbar__username m--hide">
                                                    @Url.Content(@Model.MEMBERS.FULLNAME ?? "")


                                                </span>
                                            </a>
                                            <div class="m-dropdown__wrapper">
                                                <span class="m-dropdown__arrow m-dropdown__arrow--right m-dropdown__arrow--adjust"></span>
                                                <div class="m-dropdown__inner">
                                                    <div class="m-dropdown__header m--align-center">
                                                        <div class="m-card-user m-card-user--skin-light">
                                                            <div class="m-card-user__pic">
                                                                @*<img src="~/Content/images/teacher/user1.jpg" class="m--img-rounded m--marginless" alt="" />*@
                                                                <img src="..\\@Url.Content(@Model.MEMBERS.PROFILE_IMG_URL ?? "\\Content\\images\\Blank-Profile.jpg" )" class="m--img-rounded m--marginless" alt="" />
                                                            </div>
                                                            <div class="m-card-user__details">
                                                                <span class="m-card-user__name m--font-weight-500">
                                                                    @Url.Content(@Model.MEMBERS.FULLNAME ?? "")
                                                                </span>
                                                                <a href="#" class="m-card-user__email m--font-weight-300 m-link">
                                                                    @Url.Content(@Model.MEMBERS.MEMBER_LOGON.FirstOrDefault().USER_NAME)

                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="m-dropdown__body">
                                                        <div class="m-dropdown__content">
                                                            <ul class="m-nav m-nav--skin-light">
                                                                <li class="m-nav__section m--hide">
                                                                    <span class="m-nav__section-text">Section</span>
                                                                </li>
                                                                <li class="m-nav__item">
                                                                    <a href="#" class="m-nav__link">
                                                                        <i class="m-nav__link-icon flaticon-profile-1"></i>
                                                                        <span class="m-nav__link-title">
                                                                            <span class="m-nav__link-wrap">
                                                                                <span class="m-nav__link-text">My Profile</span>
                                                                                <span class="m-nav__link-badge"><span class="m-badge m-badge--success">2</span></span>
                                                                            </span>
                                                                        </span>
                                                                    </a>
                                                                </li>
                                                                <li class="m-nav__item">
                                                                    <a href="@Url.Action("getteacher","student")" class="m-nav__link">
                                                                        <i class="m-nav__link-icon flaticon-share"></i>
                                                                        <span class="m-nav__link-text">หน้าแรก</span>
                                                                    </a>
                                                                </li>
                                                                <li class="m-nav__separator m-nav__separator--fit">
                                                                </li>
                                                                <li class="m-nav__item">
                                                                    <a href="@Url.Action("LogOff","account")" class="btn m-btn--pill    btn-secondary m-btn m-btn--custom m-btn--label-brand m-btn--bolder">Logout</a>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </li>

                                    </ul>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div id="m_header_topbar" class="m-topbar  m-stack m-stack--ver m-stack--general">
                                <div class="m-stack__item m-topbar__nav-wrapper">
                                    <ul class="m-topbar__nav m-nav m-nav--inline">
                                        <li class="m-nav__item">
                                            <a href="@Url.Action("login", "account")" class="m-nav__link">
                                                <span class="m-nav__link-text" style="color:white">Login</span>
                                            </a>
                                        </li>
                                        <li class="m-nav__item">
                                            <a href="@Url.Action("register", "account")" class="m-nav__link">
                                                <span class="m-nav__link-text" style="color:white">Register</span>
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        }
                        <!-- END: Topbar -->
                    </div>
                </div>
            </div>
        </header>

        <!-- END: Header -->
        <!-- begin::Body -->
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
            <!-- BEGIN: Left Aside -->
            <!-- END: Left Aside -->
            <div class="m-grid__item m-grid__item--fluid m-wrapper">
                <!-- BEGIN: Subheader -->
                <!-- END: Subheader -->
                @*<div class="m-subheader ">
                        <div class="d-flex align-items-center">
                            <div class="mr-auto">
                                <h3 class="m-subheader__title ">My Profile</h3>
                            </div>
                        </div>
                    </div>*@

                <div class="m-content">

                    <div class="row">

                        <div class="col-xl-3 col-lg-4">
                            @using (Html.BeginForm("getteacher", "student", FormMethod.Post))
                            {
                                <div class="m-portlet m-portlet--tab">
                                    <div class="m-portlet__body">
                                        <div class="form-group m-form__group">
                                            <label for="exampleSelect1">ประเภท</label>
                                            <select class="form-control m-input m-input--air m-input--pill" id="exampleSelect1">
                                                <option>ครู</option>
                                                <option>คอร์ส</option>
                                            </select>
                                            <label for="exampleSelect1">วิชา</label>
                                            @Html.DropDownListFor(m => m.SEARCH_TEACHER_MODEL.LIST_COURSE, new SelectList(Model.SEARCH_TEACHER_MODEL.LIST_COURSE), new { @name = "COURSE", @class = "form-control m-input m-input--air m-input--pill" })
                                        </div>
                                        <div class="form-group m-form__group">
                                            <label for="exampleSelect1">จังหวัด</label>
                                            @Html.DropDownListFor(m => m.SEARCH_TEACHER_MODEL.LIST_PROVINCE, new SelectList(Model.SEARCH_TEACHER_MODEL.LIST_PROVINCE), new { @class = "form-control m-input m-input--air m-input--pill" })
                                        </div>
                                        <div class="form-group m-form__group">
                                            <label for="exampleSelect1">เขต</label>
                                            <select class="form-control m-input m-input--air m-input--pill" name="AREA" id="exampleSelect1">
                                                <option>ปทุมวัน</option>
                                                <option>บางนา</option>

                                            </select>
                                        </div>
                                        <div class="m-form__group form-group">
                                            <label for="">ประเภทการเรียน</label>
                                            <div class="m-radio-list">
                                                <label class="m-radio">
                                                    <input type="radio" name="TEACH_TYPE" checked="checked" value="1"> เรียนเดียว
                                                    <span></span>
                                                </label>
                                                <label class="m-radio">
                                                    <input type="radio" name="TEACH_TYPE" value="2"> เรียนกลุ่ม
                                                    <span></span>
                                                </label>

                                            </div>
                                        </div>
                                        <div class="m-form__group form-group">
                                            <label for="">สถานที่</label>
                                            <div class="m-checkbox-list">
                                                <label class="m-checkbox">
                                                    <input type="checkbox" name="TEACH_LOCATE"> เรียนที่บ้าน
                                                    <span></span>
                                                </label>
                                                <label class="m-checkbox">
                                                    <input type="checkbox" name="TEACH_LOCATE"> เรียนนอกสถานที่
                                                    <span></span>
                                                </label>
                                                <label class="m-checkbox">
                                                    <input type="checkbox" name="TEACH_LOCATE"> เรียนที่สถานที่ครู
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>

                                        <div class="m-form__group form-group">
                                            <label for="">เพศ</label>
                                            <div class="m-checkbox-list">
                                                <label class="m-checkbox">
                                                    <input type="checkbox" value="1" name="SEARCH_TEACHER_MODEL.TEACH_GENDER"> ชาย
                                                    <span></span>
                                                </label>
                                                <label class="m-checkbox">
                                                    <input type="checkbox" value="2" name="SEARCH_TEACHER_MODEL.TEACH_GENDER"> หญิง
                                                    <span></span>
                                                </label>

                                            </div>
                                        </div>

                                        <div class="m-form__group form-group">
                                            <label for="">ระดับผู้เรียน</label>
                                            <div class="m-checkbox-list">
                                                <label class="m-checkbox">
                                                    <input type="checkbox" value="1" name="STUDENT_LEVEL"> เริ่มต้น
                                                    <span></span>
                                                </label>
                                                <label class="m-checkbox">
                                                    <input type="checkbox" value="2" name="STUDENT_LEVEL"> ปานกลาง
                                                    <span></span>
                                                </label>
                                                <label class="m-checkbox">
                                                    <input type="checkbox" value="3" name="STUDENT_LEVEL"> ขั้นสูง
                                                    <span></span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="m-portlet__foot">
                                            <div class="row align-items-center">
                                                <div class="col-lg-12">
                                                    <button type="submit" class="btn btn-brand m-btn--pill m-btn--air">Search</button>
                                                    <button type="submit"class="btn btn-secondary m-btn--pill m-btn--air" name="SEARCH_TEACHER_MODEL" value="">Search All</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </div>

                        <div class="col-xl-9 col-lg-8">

                            @if (Model.LIST_CUSTOM_MEMBERS != null)
                            {
                                foreach (var item in Model.LIST_CUSTOM_MEMBERS)
                                {

                                    <div class="m-portlet m-portlet--rounded">

                                        <div class="m-portlet__body">
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <div class="m-card-profile__pic-wrapper">
                                                        <img src="..//@Url.Content(item.PROFILE_IMG_URL ?? "\\Content\\images\\Blank-Profile.jpg")" class="img-thumbnail">
                                                    </div>
                                                    <input data-val="true" data-val-number="The field AUTO_ID must be a number." data-val-required="The AUTO_ID field is required." id="AUTO_ID" name="MEMBERS.AUTO_ID" type="hidden" value="133">
                                                </div>
                                                <div class="col-md-8">
                                                    <p><i class="fa fa-male"></i>&nbsp ชื่อ : @Url.Content(item.FULLNAME)</p><br>
                                                    <p><i class="fa fa-plus"></i>&nbsp คอร์สที่สอน : @Url.Content(item.COURSE)</p><br>
                                                    @*<p><i class="fa fa-book"></i>&nbsp หมวดหมู่ : @Url.Content(item.CATEGORY)</p><br>*@
                                                    <p><i class="fa fa-book"></i>&nbsp สถานที่ : @Url.Content(item.LOCATION)</p><br>
                                                    <p><i class="fa fa-address-card"></i>&nbsp เกี่ยวกับผู้สอน : @Url.Content(item.ABOUT)</p><br>
                                                    @if (Session["logon"] == null)
                                                    {
                                                        <a href="#" class="btn btn-brand m-btn m-btn--custom m-btn--icon m-btn--pill m-btn--air" data-toggle="modal" data-target="#m_modal_6">
                                                            <span>
                                                                <i class="fa fa-check-square"></i>
                                                                <span>Accept Teacher</span>
                                                            </span>
                                                        </a>

                                                        @*<button type="button" class="btn btn-focus" data-toggle="modal" data-target="#m_modal_6">Launch Modal</button>*@

                                                    using (Html.BeginForm("ChooseTeacherLogin", "account", FormMethod.Post))
                                                    {
                                                        <div class="modal fade" id="m_modal_6" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" style="display: none;" aria-hidden="true">
                                                            <div class="modal-dialog modal-dialog-centered" role="document">
                                                                <div class="modal-content">
                                                                    <div class="modal-header">
                                                                        <h5 class="modal-title" id="exampleModalLongTitle">Login</h5>
                                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                            <span aria-hidden="true">×</span>
                                                                        </button>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        <div class="form-group m-form__group">
                                                                            <label for="exampleInputEmail1">Username</label>
                                                                            <input type="email" class="form-control m-input" name="USER_NAME" aria-describedby="emailHelp" placeholder="Enter email">
                                                                            @Html.ValidationMessageFor(m => m.MEMBER_LOGON.USER_NAME, "", new { @class = "text-danger" })
                                                                        </div>
                                                                        <div class="form-group m-form__group">
                                                                            <label for="exampleInputPassword1">Password</label>
                                                                            <input type="password" class="form-control m-input" name="PASSWORD" placeholder="Password" />
                                                                        </div>
                                                                    </div>
                                                                    <div class="modal-footer">
                                                                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                                                        <button type="submit" class="btn btn-brand m-btn m-btn--custom m-btn--icon m-btn--pill m-btn--air">Login</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        <button class="btn btn-brand m-btn m-btn--custom m-btn--icon m-btn--pill m-btn--air" data-toggle="modal" data-target="#@("MODAL_ACCEPT_TEACHER" + item.AUTO_ID)">
                                                            <span>
                                                                <i class="fa fa-check-square"></i>
                                                                <span>Accept Teacher</span>
                                                            </span>
                                                        </button>

                                                        using (Html.BeginForm("AcceptTeacher", "student", FormMethod.Post))
                                                        {
                                                            <div class="modal fade" id="@("MODAL_ACCEPT_TEACHER" + item.AUTO_ID)" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" style="display: none" aria-hidden="true">
                                                                <div class="modal-dialog modal-lg" role="document">
                                                                    <div class="modal-content">
                                                                        <div class="modal-body">
                                                                            <div class="m-portlet__body">
                                                                                <div class="m-demo__preview m-demo__preview--badge">

                                                                                    <h4 class="m--font-bold m--font-bold">
                                                                                        <span>
                                                                                            <i class="fa fa-address-card"></i>
                                                                                            @Html.Hidden("COURSE_ID", item.COURSE_ID)
                                                                                            @Html.Hidden("COURSE_NAME", item.COURSE)
                                                                                            @Html.Hidden("MEMBER_ROLE_ID", item.MEMBER_ROLE_ID)
                                                                                            @Html.HiddenFor(m => m.MEMBERS.AUTO_ID)
                                                                                            <span> คุณจะเลือกครู @Url.Content(item.FULLNAME) ใช่หรือไม่ ?</span>
                                                                                        </span>

                                                                                    </h4>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="modal-footer">
                                                                            <button class="btn m-btn--pill m-btn--air btn-primary" type="submit" name="AcceptStudent" value="@item.REGIS_COURSE_ID">
                                                                                <span>
                                                                                    <i class="fa fa-check-square"></i>
                                                                                    <span>Confirm</span>
                                                                                </span>
                                                                            </button>
                                                                            <button type="button" class="btn m-btn--pill m-btn--air btn-secondary m-btn m-btn--custom" data-dismiss="modal">Close</button>
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        }
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }
                            }


                        </div>
                    </div>



                </div>

            </div>
        </div>

        <!-- end:: Body -->
        <!-- begin::Footer -->
        <footer class="m-grid__item		m-footer ">
            <div class="m-container m-container--fluid m-container--full-height m-page__container">
                <div class="m-stack m-stack--flex-tablet-and-mobile m-stack--ver m-stack--desktop">
                    <div class="m-stack__item m-stack__item--left m-stack__item--middle m-stack__item--last">
                        <span class="m-footer__copyright">
                            2018 © www.coachme.asia<a href="www.coachme.asia" class="m-link"></a>
                        </span>
                    </div>
                    <div class="m-stack__item m-stack__item--right m-stack__item--middle m-stack__item--first">
                        <ul class="m-footer__nav m-nav m-nav--inline m--pull-right">
                            <li class="m-nav__item">
                                <a href="#" class="m-nav__link">
                                    <span class="m-nav__link-text">About</span>
                                </a>
                            </li>
                            <li class="m-nav__item">
                                <a href="#" class="m-nav__link">
                                    <span class="m-nav__link-text">Privacy</span>
                                </a>
                            </li>
                            <li class="m-nav__item">
                                <a href="#" class="m-nav__link">
                                    <span class="m-nav__link-text">T&amp;C</span>
                                </a>
                            </li>
                            <li class="m-nav__item">
                                <a href="#" class="m-nav__link">
                                    <span class="m-nav__link-text">Purchase</span>
